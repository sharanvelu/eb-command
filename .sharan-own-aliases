# Directories
alias app="cd /var/www/current/"

# Laravel
alias art="php artisan"
alias tinker="php artisan tinker"
alias migrate="php artisan migrate"
alias mf="php artisan migrate:fresh"
alias mr="php artisan migrate:rollback"
alias mr1="php artisan migrate:rollback --step=1"
alias seed="php artisan db:seed"
alias clr="php artisan optimize:clear"

# ENV Variables
alias envvar="cd /var/app/current; sudo chmod 777 /opt/elasticbeanstalk/deployment/env; export $(xargs -a /opt/elasticbeanstalk/deployment/env)"
